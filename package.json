{
  "name": "generator-jhipster-nav-element",
  "version": "3.1.2",
  "description": "Client Side Angular Components For your JHipster App",
  "keywords": [
    "scaffold",
    "angular",
    "generator",
    "yeoman-generator",
    "jhipster",
    "jhipster-module"
  ],
  "homepage": "https://github.com/vivekmore/generator-jhipster-nav-element",
  "author": {
    "name": "Vivek More",
    "email": "vivz.thats.me@gmail.com",
    "url": "https://github.com/vivekmore"
  },
  "engines": {
    "node": ">=12.18.3",
    "npm": ">=6.13.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekmore/generator-jhipster-nav-element.git"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "generator-jhipster": "6.10.3",
    "kind-of": ">=6.0.3",
    "lodash": "4.17.20",
    "minimist": "1.2.5",
    "mkdirp": "1.0.4",
    "semver": "7.3.2",
    "shelljs": "0.8.4",
    "tabtab": "3.0.2",
    "yeoman-generator": "4.12.0"
  },
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "@types/yeoman-assert": "3.1.1",
    "coveralls": "3.1.0",
    "eslint": "7.10.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "fs": "0.0.1-security",
    "fs-extra": "9.0.1",
    "istanbul": "0.4.5",
    "mocha": "8.1.3",
    "mocha-lcov-reporter": "1.3.0",
    "rimraf": "3.0.0",
    "typescript": "3.7.2",
    "typescript-tslint-plugin": "0.5.5",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "3.0.0"
  },
  "scripts": {
    "build": "tsc",
    "rebuild": "npm run clean && npm run build",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib coverage",
    "prepare": "npm run rebuild",
    "pretest": "eslint .",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "npm run test:unit",
    "test:unit": "mocha test/**/test.js generators/**/*.spec.js",
    "preversion": "npm i && npm test",
    "postversion": "git push --tags",
    "bump-patch": "npm version patch",
    "bump-minor": "npm version minor",
    "bump-major": "npm version major",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha test/**/test.js generators/**/*.spec.js",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec test/**/test.js generators/**/*.spec.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vivekmore/generator-jhipster-nav-element/issues"
  }
}
